{
  "name": "phantron-obfuscator",
  "version": "2.0.8",
  "description": "Professional code obfuscation CLI with auto-tuning, multi-file support, and comprehensive HTML reporting",
  "main": "cli/index.js",
  "files": [
    "cli/",
    "config/",
    "examples/",
    "release/",
    "README.md",
    "LICENSE",
    "INSTALLATION.md",
    "DISTRIBUTION.md",
    "QUICK_REFERENCE_STANDALONE.md",
    "WSL_CLI_USAGE.md",
    "phantron-wsl.bat"
  ],
  "bin": {
    "phantron": "./cli/index.js",
    "phantron-obfuscate": "./cli/index.js"
  },
  "scripts": {
    "start": "node cli/index.js",
    "test": "node cli/index.js --help",
    "build": "make -C build phantron-llvm-obfuscator",
    "build:standalone": "bash scripts/build/build-standalone.sh",
    "build:standalone:win": "powershell -ExecutionPolicy Bypass -File scripts/build/build-standalone.ps1",
    "demo": "./demo_auto_tune.sh",
    "docs": "cat docs/AUTO_TUNING.md"
  },
  "pkg": {
    "assets": [
      "build/**/*",
      "config/**/*.yaml",
      "cli/**/*.js"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-linux-arm64",
      "node18-win-x64",
      "node18-win-arm64"
    ],
    "outputPath": "dist"
  },
  "keywords": [
    "obfuscation",
    "security",
    "code-protection",
    "auto-tuning",
    "optimization",
    "llvm",
    "phantron",
    "c",
    "cpp",
    "reverse-engineering-protection",
    "binary-protection"
  ],
  "author": {
    "name": "Phantron Team - ELEVATE_1",
    "email": "support@phantron.dev"
  },
  "contributors": [
    {
