      "name": "Sanjana Suresh"
    }
  ],
  "license": "MIT",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeuralNinja110/SIH25_2.git"
  },
  "homepage": "https://github.com/NeuralNinja110/SIH25_2#readme",
  "bugs": {
    "url": "https://github.com/NeuralNinja110/SIH25_2/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^9.4.1",
    "figures": "^3.2.0",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "listr": "^0.14.3",
    "ora": "^5.4.1",
    "update-notifier": "^6.0.2"
  },
  "devDependencies": {
    "eslint": "^8.30.0"
  },
  "preferGlobal": true,
  "os": [
